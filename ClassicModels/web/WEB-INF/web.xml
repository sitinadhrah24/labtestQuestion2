<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    
    <servlet>
        <servlet-name>GetStaff</servlet-name>
        <servlet-class>com.servlet.GetStaff</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>GetStaffById</servlet-name>
        <servlet-class>com.servlet.GetStaffById</servlet-class>
    </servlet>
    
    
   
    <servlet-mapping>
        <servlet-name>GetStaff</servlet-name>
        <url-pattern>/GetStaff</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>GetStaffById</servlet-name>
        <url-pattern>/GetStaffById</url-pattern>
    </servlet-mapping>
    
   
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
